{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "http-server .vercel/output/static -p 4321 -s",
    "astro": "astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint \"src/**/*.{js,ts,astro}\" \"**/*.md\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,astro}\" \"**/*.md\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,astro}\" \"**/*.md\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,astro}\" \"**/*.md\""
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/vercel": "^9.0.4",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@tailwindcss/vite": "4.1.18",
    "@vercel/speed-insights": "^1.3.1",
    "astro": "5.17.1",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@eslint/markdown": "7.5.1",
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.2",
    "@typescript-eslint/parser": "8.55.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "10.0.0",
    "eslint-plugin-astro": "1.5.0",
    "globals": "17.3.0",
    "happy-dom": "^20.6.1",
    "http-server": "^14.1.1",
    "jiti": "2.6.1",
    "vitest": "^4.0.18"
  }
}
