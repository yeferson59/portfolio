{
  "name": "frontend",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "http-server .vercel/output/static -p 4321 -s",
    "astro": "astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint \"src/**/*.{js,ts,astro}\" \"**/*.md\"",
    "lint:fix": "eslint \"src/**/*.{js,ts,astro}\" \"**/*.md\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,astro}\" \"**/*.md\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,astro}\" \"**/*.md\""
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/vercel": "^8.2.10",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@tailwindcss/vite": "4.1.16",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "5.14.7",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.15",
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@eslint/markdown": "7.4.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.1",
    "@typescript-eslint/parser": "8.46.2",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "eslint": "9.38.0",
    "eslint-plugin-astro": "1.4.0",
    "globals": "16.4.0",
    "happy-dom": "^20.0.8",
    "http-server": "^14.1.1",
    "jiti": "2.6.1",
    "vitest": "^4.0.4"
  }
}
